<cheprooPlusPlusInputFile name="cheproo_input">

	<cheprooPlusPlus name="simulation1">
		<function name="speciateWaters" isSecOrdDerivCalc="false" writeInfo="false">
			<ref type = "water" ID="1">water_injected</ref>	
		</function>	
	</cheprooPlusPlus>
	
	<globalChemicalSystem buildOption="readFromMaster25" thermodynamicDatabase="completemaster25.xml">

		<phase name="aqueous1" type="aqueousDebyeHuckel" model="aqueous"> 
			<ref type="species">na+</ref>
			<ref type="species">so4-2</ref>
			<ref type="species">ca+2</ref>
			<ref type="species">h+</ref>
			<ref type="species">naso4</ref>
			<ref type="species">caso4(aq)</ref>
			<ref type="species">h2o</ref>
			<ref type="species">oh-</ref>
		</phase>
 		<localChemicalSystems>
			<localChemicalSystem name="local1" type="localChemicalSystemConstPrimary" maxRelativeError="1e-5" maxResidual="1e-10" maxIterNum="100">
				<ref type="primarySpecies">na+</ref>
				<ref type="primarySpecies">so4-2</ref>
				<ref type="primarySpecies">ca+2</ref>
				<ref type="primarySpecies">h+</ref>
				<ref type="primarySpecies">oh-</ref>
			</localChemicalSystem>
		</localChemicalSystems>

	</globalChemicalSystem>
	
	<proostList type = "water">	
		<water ID="1" name="water_injected" temp="25.00">  <!--agua que se inyecta-->
			<ref type="species" iCon="ctot" cGuess="4.0e-5" cTot="4.0e-4" constraint="">na+</ref>
			<ref type="species" iCon="ctot" cGuess="1.0e-4" cTot="1.0e-3" constraint="">so4-2</ref>
			<ref type="species" iCon="ctot" cGuess="8.0e-5" cTot="8.0e-4" constraint="dolomite">ca+2</ref>
			<ref type="species" iCon="ctot" cGuess="1.0e-6" cTot="1.0e-7" constraint="dolomite">h+</ref>	
			<ref type="species" iCon="ctot" cGuess="1.0e-6" cTot="1.0e-7" constraint="dolomite">oh-</ref>
		</water>
	 </proostList>

</cheprooPlusPlusInputFile>