<cheprooPlusPlusInputFile name="cheproo_input">

	<cheprooPlusPlus name="simulation1">
		<function name="speciateWaters" isSecOrdDerivCalc="false" writeInfo="false">
			<ref type="chemicalComposition" ID="1">water_injected</ref>	
		</function>	
	</cheprooPlusPlus>
	
	<globalChemicalSystem buildOption="readFromMasterTemp" thermodynamicDatabase="mastertemp.xml">

		<phase name="aqueous1" type="aqueousDebyeHuckel" model="aqueous"> 
			<ref type="species">na+</ref>
			<ref type="species">so4-2</ref>
			<ref type="species">ca+2</ref>
			<ref type="species">naso4-</ref>
			<ref type="species">caso4(aq)</ref>
		</phase>
 		<localChemicalSystems>
			<localChemicalSystem name="local1" type="localChemicalSystemConstPrimary" maxRelativeError="1e-5" maxResidual="1e-10" maxIterNum="100"/>
		</localChemicalSystems>

	</globalChemicalSystem>
	
	<proostList type = "chemicalComposition">	
		<chemicalComposition ID="1" name="water_injected" temp="25.00">  <!--agua que se inyecta-->
		
			<ref type="firstGuess" cGuess="4.0e-5" primarySpecies="na+"/>
			<ref type="firstGuess" cGuess="1.0e-4" primarySpecies="so4-2"/>
			<ref type="firstGuess" cGuess="8.0e-5" primarySpecies="ca+2"/>
			
			<ref type="constraint" iCon="ctot" value="4.0e-4" name="na+"/>
			<ref type="constraint" iCon="ctot" value="1.0e-3" name="so4-2"/>
			<ref type="constraint" iCon="ctot" value="8.0e-4" name="ca+2"/>
			
			<ref type="phaseContent" name="aqueous1" model="aqueous" value="1.0"/>
			
		</chemicalComposition>
	 </proostList>

</cheprooPlusPlusInputFile>