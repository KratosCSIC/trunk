<cheprooPlusPlusInputFile name="cheproo_input">

	<cheprooPlusPlus name="simulation1">
		<function name="speciateWaters" isSecOrdDerivCalc="false" writeInfo="false">
			<ref type = "chemicalComposition" ID="1">water_injected</ref>	
		</function>	
	</cheprooPlusPlus>
	
	<globalChemicalSystem buildOption="readFromMasterTemp" thermodynamicDatabase="mastertemp.xml">

		<phase name="aqueous1" type="aqueousDebyeHuckel" model="aqueous"> 
			<ref type="species">fe+2</ref>
			<ref type="species">h+</ref>
			<ref type="species">oh-</ref>
			<ref type="species">fe+3</ref>
			<ref type="species">o2</ref>
			<ref type="species">fe(oh)3</ref>
			<ref type="species">fe(oh)2</ref>
			<ref type="species">h2o</ref>
		</phase>
		<phase name="mineral1" type="mineralPure" model="mineral"> 
			<ref type="species">feooh(s)</ref>
		</phase>
		
		<localChemicalSystems>
			<localChemicalSystem name="local1" type="localChemicalSystemSaaltink" maxRelativeError="1e-5" maxResidual="1e-10" maxIterNum="100"/>
		</localChemicalSystems>
		
	</globalChemicalSystem>
	
	<proostList type="chemicalComposition">	
		<chemicalComposition ID="1" name="water_injected" temp="25.00">  <!--agua que se inyecta-->
		
			<ref type="firstGuess" cGuess="4.0e-5" primarySpecies="fe+2"/>
			<ref type="firstGuess" cGuess="1.0e-4" primarySpecies="h+"/>
			<ref type="firstGuess" cGuess="8.0e-5" primarySpecies="oh-"/>
			<ref type="firstGuess" cGuess="1.0e-6" primarySpecies="fe+3"/>
			
			<ref type="constraint" iCon="ctot" value="4.0e-4" name="fe+2"/>
			<ref type="constraint" iCon="ctot" value="1.0e-3" name="h+"/>
			<ref type="constraint" iCon="ctot" value="8.0e-4" name="oh-"/>
			<ref type="constraint" iCon="chgbal" value="1.0e-7" name="fe+3"/>
			
			<ref type="phaseContent" name="aqueous1" model="aqueous" value="0.1"/>
			<ref type="phaseContent" name="mineral1" model="mineral" value="0.1"/>
			
		</chemicalComposition>
	 </proostList>

</cheprooPlusPlusInputFile>