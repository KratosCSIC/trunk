			<ref type="species" iCon="ctot" cGuess="1.0e-3" cTot="4.47e-3" constraint="dolomite">ca+2</ref>
			<ref type="species" iCon="ctot" cGuess="0.0" cTot="4.47e-3" constraint="dolomite">oh-</ref>			
		</water>
